		"@microsoft/tsdoc": "0.14.2"
	},
	"devDependencies": {
		"@types/node": "16.18.29",
		"cross-env": "^7.0.3",
		"eslint": "^8.40.0",
		"eslint-config-neon": "^0.1.47",
		"eslint-formatter-pretty": "^5.0.0",
		"prettier": "^2.8.8",
